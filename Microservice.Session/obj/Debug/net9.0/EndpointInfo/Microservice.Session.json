{
  "openapi": "3.0.4",
  "info": {
    "title": "Microservice.Session",
    "version": "1.0"
  },
  "paths": {
    "/api/Session/create": {
      "post": {
        "tags": [
          "Session"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SessionRequestDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SessionRequestDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SessionRequestDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Session/end-session": {
      "post": {
        "tags": [
          "Session"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Session/log-activity": {
      "post": {
        "tags": [
          "Session"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ActivityLogDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ActivityLogDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ActivityLogDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ActivityLogDto": {
        "type": "object",
        "properties": {
          "user_Id": {
            "type": "string",
            "nullable": true
          },
          "action_Type": {
            "type": "string",
            "nullable": true
          },
          "product_Id": {
            "type": "string",
            "nullable": true
          },
          "category_Id": {
            "type": "string",
            "nullable": true
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "url": {
            "type": "string",
            "nullable": true
          },
          "referrerUrl": {
            "type": "string",
            "nullable": true
          },
          "metadata": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "nullable": true
          },
          "request_Method": {
            "type": "string",
            "nullable": true
          },
          "response_Code": {
            "type": "integer",
            "format": "int32"
          },
          "success_Flag": {
            "type": "boolean"
          },
          "response_Time": {
            "type": "number",
            "format": "double"
          },
          "time_Stamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "DeviceInfoDto": {
        "type": "object",
        "properties": {
          "fingerprint": {
            "type": "string",
            "nullable": true
          },
          "browser": {
            "type": "string",
            "nullable": true
          },
          "device_Type": {
            "type": "string",
            "nullable": true
          },
          "os": {
            "type": "string",
            "nullable": true
          },
          "language": {
            "type": "string",
            "nullable": true
          },
          "screen_Resolution": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SessionRequestDto": {
        "type": "object",
        "properties": {
          "user_Id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "ip_Address": {
            "type": "string",
            "nullable": true
          },
          "referrerUrl": {
            "type": "string",
            "nullable": true
          },
          "device": {
            "$ref": "#/components/schemas/DeviceInfoDto"
          }
        },
        "additionalProperties": false
      }
    }
  }
}